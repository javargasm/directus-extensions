{
  "name": "directus-extension-mailer-operation",
  "version": "0.1.1",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-operation"
  ],
  "directus:extension": {
    "type": "operation",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "source": {
      "app": "src/app.ts",
      "api": "src/api.ts"
    },
    "host": "^10.3.3"
  },
  "scripts": {
    "build": "directus-extension build",
    "watch": "directus-extension build -w --no-minify"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.18.0",
    "@types/node": "^18.7.18",
    "shared": "workspace:*",
    "typescript": "^4.8.3",
    "vue": "^3.2.39"
  },
  "eslintConfig": {
    "extends": [
      "shared/.eslintrc.json"
    ]
  },
  "prettier": "shared/.prettierrc.json"
}
