{
  "name": "directus-extension-firebase-messaging-operation",
  "description": "Custom operation to send messages with Firebase",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-operation"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "operation",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "source": {
      "app": "src/app.ts",
      "api": "src/api.ts"
    },
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "watch": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "dependencies": {
    "firebase-admin": "^11.0.1"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.18.0",
    "@types/node": "^18.7.18",
    "typescript": "^4.8.3",
    "vue": "^3.2.39"
  }
}
